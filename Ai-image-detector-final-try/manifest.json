{
  "manifest_version": 3,
  "name": "AI Image Detector",
  "version": "1.0.0",
  "description": "Detect if an image is AI-generated or real using machine learning",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage"],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": ["model/*"],
      "matches": ["<all_urls>"]
    }
  ]
}